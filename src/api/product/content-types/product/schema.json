{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product_id": {
      "type": "uid",
      "required": true
    },
    "product_name": {
      "type": "string",
      "required": true,
      "maxLength": 70,
      "minLength": 3
    },
    "description": {
      "type": "text",
      "required": false,
      "minLength": 3
    },
    "price": {
      "type": "float",
      "required": true
    },
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order"
    },
    "varient_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::varient.varient",
      "mappedBy": "product_id"
    },
    "imglink": {
      "type": "string",
      "required": true
    },
    "imglink2": {
      "type": "string",
      "default": "https://images.emojiterra.com/google/android-nougat/512px/1f6e1.png"
    },
    "Safety_Score": {
      "type": "integer",
      "required": true,
      "default": 73
    },
    "isNew": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "product_id"
    }
  }
}
